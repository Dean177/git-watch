    "gulp-util": "^3.0.6",
    "immutable": "^3.7.4",
    "nodegit": "^0.4.1",
    "react": "^0.13.3",
    "react-router": "^0.13.3",
    "react-skeleton": "Dean177/react-skeleton",
    "redux": "^1.0.0-alpha",
    "uniqueid": "^0.1.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "devDependencies": {
    "babel": "^5.6.14",
    "babelify": "^6.1.2",
    "browserify": "^10.2.4",
    "chai": "^3.0.0",
    "gulp-babel": "^5.1.0",
    "gulp-concat": "^2.6.0",
    "gulp-mocha": "^2.1.2",
    "loca": "^1.0.1",
    "mocha": "^2.2.5",
    "mocha-junit-reporter": "^1.5.0",
    "node-webkit-builder": "^1.0.12",
    "watchify": "^3.2.3"
  }
