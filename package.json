{
  "name": "git-watch",
  "version": "0.0.1",
  "description": "Be notified when remote branches are updated",
  "repository": "git@github.com:Dean177/git-watch.git",
  "main": "dist/index.html",
  "author": "Dean Merchant",
  "license": "ISC",
  "engines": {
    "nwjs": "0.12.2"
  },
  "window": {
    "icon": "app/images/icon.png",
    "toolbar": true,
    "frame": true,
    "width": 1000,
    "height": 700,
    "position": "center",
    "resizable": true
  },
  "webkit": {
    "plugin": false
  },
  "scripts": {
    "start": "watchify app/jsx/index.jsx -o app/bundle.js -v",
    "test": "node ./node_modules/gulp/bin/gulp.js test:junit",
    "app": "nwbuild --run .",
    "build": "./build/build",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "del": "^1.2.0",
    "envify": "^3.4.0",
    "fs-extra": "^0.22.1",
